datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Room {
  id          Int      @id @default(autoincrement())
  videoUrl    String?
  currentTime Float? // In seconds from the start of the video
  createdAt   DateTime @default(now())
}
